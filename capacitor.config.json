{
  "appId": "com.bottle.golfpeople",
  "appName": "GolfPeople",
  "webDir": "dist",
  "server": {
    "androidScheme": "https",
    "iosScheme": "https",
    "allowNavigation": [
      "*.supabase.co",
      "kauth.kakao.com",
      "accounts.google.com",
      "golf-people.vercel.app"
    ]
  },
  "ios": {
    "contentInset": "always",
    "backgroundColor": "#0D0D0D",
    "preferredContentMode": "mobile"
  },
  "android": {
    "allowMixedContent": true,
    "backgroundColor": "#0D0D0D"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0,
      "launchAutoHide": true,
      "launchFadeOutDuration": 0,
      "backgroundColor": "#0D0D0D",
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP",
      "showSpinner": false,
      "splashFullScreen": true,
      "splashImmersive": true
    },
    "StatusBar": {
      "style": "DARK",
      "backgroundColor": "#0D0D0D"
    },
    "Keyboard": {
      "resize": "body",
      "resizeOnFullScreen": true
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": ["phone"]
    }
  }
}
